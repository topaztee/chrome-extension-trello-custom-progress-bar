{
    "manifest_version": 2,

    "name": "rainbow checklist",
    "short_name": "blah",
    "description": "blah",
    "version": "1.0",

    "permissions": [
         "tabs",
        "storage",
      "webNavigation"
    ],
    "browser_action": {
        "default_title": "blahblah",
        "default_icon": {
          "16": "16.png",
          "32": "32.png"
        }
    },
  "background": {
    "matches": ["*://trello.com/c/*"],
    "scripts": ["background.js"],
    "persistent": false
  },

    "icons": {
        "128": "128.png",
        "16": "16.png",
        "48": "48.png"
     },

    "content_scripts": [{
        "matches": ["*://trello.com/*"],
        "js": ["content.js"]
      }]
  }
